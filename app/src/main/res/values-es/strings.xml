    <string name="random_phrases_0">Mensajes que desaparecen.\nConfianza que permanece.</string>
    <string name="random_phrases_1">Privacidad total, cero rastro.</string>
    <string name="random_phrases_2">Habla libremente.\nNo dejes nada atrás.</string>
    <string name="random_phrases_3">Sin huella.\nSin compromiso.</string>
    <string name="random_phrases_4">Privacidad reinventada.</string>
    <string name="random_phrases_5">Porque Ziphra no deja evidencia.</string>
    <string name="random_phrases_6">Habla inteligentemente.\nDesaparece más inteligentemente.</string>
    <string name="random_phrases_7">Di lo que sea.\nNo te arrepientas de nada.</string>
    <string name="random_phrases_8">Confidencial por diseño.</string>
    <string name="random_phrases_9">Para conversaciones que no deberían quedarse.</string>
    <string name="random_phrases_10">Donde la privacidad no es opcional.</string>
    <string name="random_phrases_11">Tus palabras, tus reglas.</string>
    <string name="random_phrases_12">Libertad para Hablar.\nPoder del Anonimato</string>
    <string name="random_phrases_13">Habla con audacia.\nNo dejes ninguna pista.</string>
    <string name="random_phrases_14">Solo tú decides quién recuerda.</string>
    <string name="random_phrases_15">La privacidad tiene un nuevo nombre.</string>
    <string name="random_phrases_16">Nada guardado.\nNada robado.</string>
    <string name="random_phrases_17">Encriptado.\nEfímero.\nEmpoderado.</string>
    <string name="random_phrases_18">Sin rastros.\nSolo la verdad.</string>
    <string name="random_phrases_19">Desaparece después de la entrega.</string>
    <string name="random_phrases_20">Comparte sin sacrificios.</string>
    <string name="random_phrases_21">Conversaciones sin consecuencias.</string>
    <string name="random_phrases_22">Diseñado para ti.\nHecho para una libertad sin miedo.</string>
    <string name="random_phrases_23">Un mensajero que olvida a propósito.</string>
    <string name="random_phrases_24">Habla sin dejar rastro.</string>
    <string name="random_phrases_25">Envíalo como si nunca hubiera sucedido.</string>
    <string name="random_phrases_26">La verdadera privacidad comienza aquí.</string>
    <string name="random_phrases_27">Habla.\nDesaparece.\nRepite.</string>
    <string name="random_phrases_28">No dejes rastro, solo impacto.</string>
    <string name="random_phrases_29">Cuando la privacidad no es negociable.</string>
    <string name="random_phrases_30">Conversaciones instantáneas.\nCero evidencia.</string>
    <string name="random_phrases_31">Dilo en voz alta.\nDesaparece rápido.</string>
    <string name="random_phrases_32">Palabras que se desvanecen, confianza duradera.</string>
    <string name="random_phrases_33">Mensajes que no se quedan.</string>
    <string name="random_phrases_34">Para aquellos que valoran la privacidad.</string>
    <string name="random_phrases_35">Habla extraoficialmente, siempre.</string>
    <string name="random_phrases_36">Borra el pasado, al instante.</string>
    <string name="random_phrases_37">Lo que sucede en el chat, se queda en el chat.</string>
    <string name="random_phrases_38">Sé escuchado.\nLuego desaparece.</string>
    <string name="random_phrases_39">Privado por defecto.\nSiempre.</string>
    <string name="random_phrases_40">Habla sin dejar rastro.</string>
    <string name="random_phrases_41">Olvidable por las razones correctas.</string>
    <string name="random_phrases_42">Anónimo.\nSeguro.\nProtegido.</string>
    <string name="random_phrases_43">Di lo que piensas, luego desaparece.</string>
    <string name="random_phrases_44">Mensajería a nivel fantasma.</string>
    <string name="random_phrases_45">Invisible por intención.</string>
    <string name="random_phrases_46">Sin copias.\nSin registros.\nSin preocupaciones.</string>
    <string name="random_phrases_47">Comparte sin miedo.</string>
    <string name="random_phrases_48">Habla con seguridad.\nBorra automáticamente.</string>
    <string name="random_phrases_49">Nada que filtrar.\nNada que hackear.</string>
    <string name="random_phrases_50">Conversación privada.\nConfianza pública.</string>
    <string name="random_phrases_51">Seguridad que puedes sentir.\nMensajes que no puedes encontrar.</string>
    <string name="random_phrases_52">El futuro de los mensajes olvidados.</string>
    <string name="random_phrases_53">Borra a medida que avanzas.</string>
    <string name="random_phrases_54">Desaparece con dignidad.</string>
    <string name="random_phrases_55">Porque borrar no es suficiente.</string>
    <string name="random_phrases_56">Diseñado para los preocupados por la privacidad.</string>
    <string name="random_phrases_57">Dilo.\nAprópiate.\nPiérdelo.</string>
    <string name="random_phrases_58">Adelante.\nDi lo que sea.</string>
    <string name="random_phrases_59">Cero datos dejados atrás.</string>
    <string name="random_phrases_60">Como hablar con el viento.</string>
    <string name="random_phrases_61">Habla sin filtros.\nNo dejes registro.</string>
    <string name="random_phrases_62">Habla como si nadie estuviera mirando.</string>
    <string name="random_phrases_63">Desaparecido en segundos.\nSeguro de por vida.</string>
    <string name="random_phrases_64">Porque no todo necesita ser recordado.</string>
    <string name="random_phrases_65">Di más, deja menos.</string>
    <string name="random_phrases_66">Habla con valentía.\nDesaparece limpiamente.</string>
    <string name="random_phrases_67">No solo encriptado.\Borrado.</string>
    <string name="random_phrases_68">El anonimato es la verdadera seguridad.</string>
    <string name="random_phrases_69">Nada almacenado.\nNada compartido.</string>
    <string name="random_phrases_70">Elimina el riesgo.</string>
    <string name="random_phrases_71">Habla fuerte.\nDesaparece con orgullo.</string>
    <string name="random_phrases_72">Tranquilidad en cada mensaje.</string>
    <string name="random_phrases_73">Donde los secretos permanecen secretos.</string>
    <string name="random_phrases_74">Envía con confianza.\nDesaparece con elegancia.</string>
    <string name="random_phrases_75">Nada que guardar, nada que temer.</string>
    <string name="random_phrases_76">Tus mensajes, tu estrategia de salida.</string>
    <string name="random_phrases_77">Elimina la duda.</string>
