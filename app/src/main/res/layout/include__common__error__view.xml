<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    android:clickable="false"
    android:contextClickable="false"
    android:longClickable="false">

    <LinearLayout
        android:id="@+id/common__error__view__scroll"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#B1000000"
        android:clickable="true"
        android:contextClickable="false"
        android:gravity="center"
        android:longClickable="true"
        android:orientation="vertical"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible">

        <LinearLayout
            android:id="@+id/common__error__view__content_all"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"


            android:clickable="true"
            android:contextClickable="false"
            android:foregroundTintMode="multiply"
            android:gravity="center"
            android:maxWidth="270dp"
            android:minWidth="200dp"
            android:minHeight="150dp"
            android:orientation="vertical"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:visibility="visible"
            tools:visibility="visible">

            <Button
                android:id="@+id/common__error__view__close2"
                android:layout_width="55dp"
                android:layout_height="65dp"
                android:layout_gravity="center"
                android:layout_weight="0"
                android:alpha="1"
                android:backgroundTint="#000000"
                android:textSize="8sp"
                app:cornerRadius="150dp"
                app:icon="@drawable/ic_warning_sign"
                app:iconGravity="textTop"
                app:iconPadding="0dp"
                app:iconSize="35dp"
                app:iconTint="#FFEA00" />

            <LinearLayout
                android:id="@+id/common__error__view__content_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:background="@drawable/rounderrorview"
                android:clickable="true"
                android:foregroundTint="#FFFFFF"
                android:orientation="vertical"
                android:paddingLeft="20dp"
                android:paddingTop="10dp"
                android:paddingRight="20dp"
                android:paddingBottom="10dp"
                android:visibility="visible"
                tools:visibility="visible">

                <LinearLayout
                    android:id="@+id/common__error__view__content_description"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/common__error__view__error_description"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="15dp"
                        android:layout_marginEnd="15dp"
                        android:inputType="textMultiLine"

                        android:maxHeight="300dp"
                        android:maxLines="2"
                        android:text="1"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>

                <TextView
                    android:id="@+id/common__error__view__recomendacion"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"
                    android:layout_marginTop="10dp"
                    android:inputType="textMultiLine"
                    android:maxHeight="300dp"

                    android:scrollbars="vertical"
                    android:text="1"
                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textFontWeight="5"
                    android:textSize="14sp" />

                <LinearLayout
                    android:id="@+id/common__error__view__content_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/common__error__view__error_code_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingEnd="10dp"
                        android:text="Código"
                        android:textColor="#000000"
                        android:textSize="10sp" />

                    <TextView
                        android:id="@+id/common__error__view__error_code"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textMultiLine"
                        android:maxHeight="300dp"
                        android:maxLines="2"
                        android:scrollbars="vertical"

                        android:text="1"
                        android:textColor="#000000"
                        android:textSize="10sp" />
                </LinearLayout>

                <LinearLayout
                    android:id="@+id/common__error__view__content_url"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/common__error__view__error_url_txt"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingEnd="10dp"
                        android:text="URL"
                        android:textColor="#000000"
                        android:textSize="10sp"
                        android:visibility="gone" />

                    <TextView
                        android:id="@+id/common__error__view__error_url"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:inputType="textMultiLine"
                        android:maxHeight="300dp"
                        android:scrollbars="vertical"
                        android:text="1"

                        android:textColor="#000000"
                        android:textSize="10sp" />
                </LinearLayout>

                <TextView
                    android:id="@+id/common__error__view__version"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:inputType="textMultiLine"
                    android:maxHeight="300dp"
                    android:scrollbars="vertical"
                    android:text="1"

                    android:textAlignment="center"
                    android:textColor="#000000"
                    android:textSize="10sp"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/common__error__view__stacktrace"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_marginTop="15dp"
                    android:background="@drawable/roundtextview"
                    android:backgroundTint="#FFFFFF"

                    android:inputType="textMultiLine"
                    android:maxHeight="250dp"
                    android:padding="10dp"
                    android:scrollbarAlwaysDrawHorizontalTrack="true"
                    android:scrollbarAlwaysDrawVerticalTrack="true"
                    android:scrollbars="horizontal|vertical"
                    android:scrollHorizontally="true"
                    android:text="1"
                    android:textColor="#000000"
                    android:textSize="10sp"
                    android:visibility="visible" />

                <LinearLayout
                    android:id="@+id/common__error__view__content_informar"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/common__error__view__informar"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:inputType="textMultiLine"
                        android:maxHeight="300dp"
                        android:scrollbars="vertical"

                        android:text="@string/general__inform__problem"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textSize="12sp" />

                    <TextView
                        android:id="@+id/common__error__view__contacto"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:autoLink="email"
                        android:inputType="textMultiLine"

                        android:linksClickable="true"
                        android:maxHeight="300dp"
                        android:scrollbars="vertical"
                        android:text="jorgekagiagian@gmail.com"
                        android:textAlignment="center"
                        android:textColor="#000000"
                        android:textColorLink="#27739F"
                        android:textSize="12sp" />
                </LinearLayout>

                <LinearLayout

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <Button
                        android:id="@+id/common__error__view__copy_all"
                        style="@style/botonMarquee"
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:layout_margin="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_marginBottom="0dp"
                        android:layout_weight="0"
                        android:paddingTop="0dp"
                        android:paddingBottom="0dp"
                        android:text="Copiar"
                        android:textSize="10sp"
                        app:icon="@drawable/ic_baseline_content_copy_20"
                        app:iconSize="16dp" />

                    <Space
                        android:layout_width="5dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1" />

                    <Button
                        android:id="@+id/common__error__view__share_all"
                        style="@style/botonMarquee"
                        android:layout_width="wrap_content"
                        android:layout_height="35dp"
                        android:layout_margin="0dp"
                        android:layout_marginTop="0dp"
                        android:layout_marginBottom="0dp"
                        android:layout_weight="0"
                        android:paddingTop="0dp"
                        android:paddingBottom="0dp"
                        android:text="Informar"
                        android:textSize="10sp"
                        app:icon="@drawable/ic_share22"
                        app:iconSize="16dp" />
                </LinearLayout>
                <!--                <ScrollView
                    android:id="@+id/common__error__view__info__scroll"
                    android:layout_width="match_parent"
                    android:layout_height="300dp">

